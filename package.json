{
  "name": "npm-unpublish-postfixed",
  "description": "Interactive CLI to unpublish postfixed package versions easily",
  "version": "0.0.1",
  "author": {
    "name": "Glebcha"
  },
  "main": "build/index.js",
  "files": [
    "build/**/*.js",
    "README.md"
  ],
  "repository": "https://github.com/glebcha/npm-unpublish-postfixed",
  "license": "MIT",
  "bin": {
    "unpublish-postfixed": "bin/unpublish-postfixed"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "prepublishOnly": "npm run build",
    "lint:code": "eslint src --ext .ts,.tsx",
    "fix:code": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "typegen": "npx tsc --declaration --emitDeclarationOnly --outDir build/types"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "inquirer": "8.1.2",
    "package-json": "7.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "7.3.3",
    "@types/node": "16.4.3",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fake-indexeddb": "3.1.3",
    "husky": "3.1.0",
    "lint-staged": "11.1.1",
    "typescript": "4.3.5"
  }
}
